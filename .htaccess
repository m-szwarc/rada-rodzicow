Options FollowSymLinks
RewriteEngine On
RewriteBase "/rr-new/"

RewriteRule "^files/(.*)$" "get_file.php?file=$1" [END]
RewriteRule "^api/([^./]*)(\?.*)?$" "api/$1.php" [END,QSA]
RewriteRule "^(\?.*)?$" "index.php" [END,QSA]
RewriteRule "^([^.?]*[^/](\.php)?)(\?.*)?$" "index.php?path=$1" [END,QSA]
RewriteRule "^([^.?]*)/(\.php)?(\?.*)?$" "$1$2$3" [END,R=301]